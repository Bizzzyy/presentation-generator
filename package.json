{
  "name": "presentation-generator",
  "version": "1.0.0",
  "description": "AI-powered presentation generator for account management professionals",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon src/server.ts",
    "client:dev": "cd client && npm start",
    "build": "tsc && cd client && npm run build",
    "start": "node dist/server.js",
    "install:all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.28.4",
    "pptxgenjs": "^3.12.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.16",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "presentation",
    "powerpoint",
    "ai",
    "generator",
    "account-management"
  ],
  "author": "",
  "license": "MIT"
}
